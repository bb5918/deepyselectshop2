    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.4.20",
    "react-daum-postcode": "^2.0.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^5.0.7",
    "react-tabs": "^3.1.1",
    "saslprep": "^1.0.3",
    "styled-components": "^5.2.0",
    "supports-color": "^7.1.0"
